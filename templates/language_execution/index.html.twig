{% extends 'base.html.twig' %}

{% block title %}LanguageExecution index{% endblock %}

{% block body %}
    <h1>LanguageExecution index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for language_execution in language_executions %}
            <tr>
                <td>{{ language_execution.id }}</td>
                <td>{{ language_execution.name }}</td>
                <td>
                    <a href="{{ path('app_language_execution_show', {'id': language_execution.id}) }}">show</a>
                    <a href="{{ path('app_language_execution_edit', {'id': language_execution.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_language_execution_new') }}">Create new</a>
{% endblock %}
