{% extends 'base.html.twig' %}

{% block title %}Login{% endblock %}

{% block body %}

    <h1>Login</h1>

    {{ form_start(loginForm) }}

        {{ form_row(loginForm._username) }}
        {{ form_row(loginForm._password, {
            label: 'Password'
        }) }}

        <button type="submit" class="btn">Register</button>

    {{ form_end(loginForm) }}

{% endblock %}
